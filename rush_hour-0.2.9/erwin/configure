int
main ()
{

        int i;

        if (isspace (129)) exit (EXIT_FAILURE);
        if (isspace (255)) exit (EXIT_FAILURE);
        if (toupper ('a') != 'A') exit(EXIT_FAILURE);
        if (toupper ('A') != 'A') exit(EXIT_FAILURE);
        if (tolower ('A') != 'a') exit(EXIT_FAILURE);
        if (tolower ('a') != 'a') exit(EXIT_FAILURE);

        for (i=-128; i < 255; i++) {
            (void)isspace (i);
            (void)isalpha (i);
            (void)isdigit (i);
            (void)tolower (i);
            (void)toupper (i);
        }

        exit (EXIT_SUCCESS);

  ;
  return 0;
}
