 lib/cross/err_map_symbol_stream.o lib/cross/err_map_symbol_string.o \
 lib/cross/err_map_char_p_char_p.o lib/cross/err_map_int_callback.o \
 lib/cross/err_map_int_v_stream.o lib/cross/err_map_file_stream.o \
 lib/cross/err_map_symbol_int.o lib/cross/err_map_int_char_p.o \
 lib/cross/err_v_char_const_p.o lib/cross/err_map_int_symbol.o \
